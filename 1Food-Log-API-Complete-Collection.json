                  "    if (response.data && response.data.access_token) {",
                  "        pm.collectionVariables.set('jwt_token', response.data.access_token);",
                  "    }",
                  "    if (response.data && response.data.user) {",
                  "        pm.collectionVariables.set('user_id', response.data.user.id);",
                  "    }",
