              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"health_data\": {\n    \"waist_circumference_cm\": 85,\n    \"blood_pressure_systolic\": 125,\n    \"blood_pressure_diastolic\": 85,\n    \"blood_sugar_mg_dl\": 95\n  }\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/users/profile",
